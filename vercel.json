{
  "version": 2,
  "builds": [
    {
      "src": "src/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/news_fetcher.py",
      "use": "@vercel/python"
    },
    {
      "src": "backend/playground_io.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/news",
      "dest": "/backend/news_fetcher.py"
    },
    {
      "src": "/api/process",
      "dest": "/backend/playground_io.py"
    },
    {
      "src": "/api/status",
      "dest": "/backend/playground_io.py"
    },
    {
      "src": "/api/tearsheet",
      "dest": "/backend/playground_io.py"
    },
    {
      "src": "/(.*)",
      "dest": "src/$1"
    }
  ]
}
